<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="RAW4J Logging" packages="">
    <Appenders>
        <Console name="stdout">
            <PatternLayout pattern="[%d{HH:mm:ss}] [%-5level] [%logger{36}] - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="com.jayway.jsonpath.internal.path.CompiledPath" additivity="false"/>

        <Root level="trace">
            <AppenderRef ref="stdout"/>
        </Root>
    </Loggers>
</Configuration>